package Project_4;

import static org.junit.Assert.*;

import org.junit.Test;

public class Test_Mix_UnMix {

	@Test 
	public void testProcessCommand() {
		Mix message = new Mix();
		message.setInitialMessage ("This is a secret message");
		String userMessage = message.processCommand("a a 0");
		userMessage = message.processCommand("a f 4");
		userMessage = message.processCommand("a z 7");
		userMessage = message.processCommand("a F 10");
		userMessage = message.processCommand("a a 0");
		userMessage = message.processCommand("a a 0");
		userMessage = message.processCommand("a a 0");
		assertEquals("Taaaahisf izs Fa secret message", userMessage);	}
}
